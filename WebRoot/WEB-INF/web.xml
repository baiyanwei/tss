<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>
  <servlet>
    <description>fetch task tester</description>
    <display-name>fetch task tester</display-name>
    <servlet-name>fetch</servlet-name>
    <servlet-class>sample.Fetch</servlet-class>
  </servlet>
  <servlet>
    <description>push data handler</description>
    <display-name>push data handler</display-name>
    <servlet-name>PushData</servlet-name>
    <servlet-class>sample.PushData</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>fetch</servlet-name>
    <url-pattern>/samples/fetch</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PushData</servlet-name>
    <url-pattern>/samples/push</url-pattern>
  </servlet-mapping>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
